{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "client",
      "image": "loki194/aws-client",
      "hostname": "client",
      "memory": 128
    },
    {
      "name": "nginx",
      "image": "loki194/aws-nginx",
      "hostname": "nginx",
      "essential": true,
      "memory": 128,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 80
        }
      ],
      "links": [
        "client"
      ]
    }
  ]
}